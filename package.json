{
  "name": "bsky-cli",
  "version": "1.0.0",
  "description": "A fancy terminal client for Bluesky",
  "main": "dist/index.js",
  "bin": {
    "bsky": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.tsx",
    "watch": "tsx watch src/index.tsx",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bluesky",
    "terminal",
    "cli",
    "atproto"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@atproto/api": "^0.15.17",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "ink": "^6.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
